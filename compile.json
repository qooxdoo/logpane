{
   "$schema": "https://raw.githubusercontent.com/qooxdoo/qooxdoo-compiler/master/resource/schema/v1/compile.json",
   "environment": {
    "qx.icontheme": "Tango"
   },
	"applications": [
		{
			"class": "qxl.logpane.Application",
			"theme": "qx.theme.Modern",
			"name": "logview",
			"title": "Qooxdoo Log View"
		}
	],
	/** Targets */
	"targets": [
		{
			"type": "source",
			"outputPath": "compiled/source"
		},
		{
			"type": "build",
			"outputPath": "compiled/build"
		}
	],
	"defaultTarget": "source",
	"locales": [
		"en", "de", "ro", "sv"
	],
	"serve": {
	   "listenPort": 8080
	}
}