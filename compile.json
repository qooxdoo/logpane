{
   "environment": {
    "qx.icontheme": "Tango"
   },
	"applications": [
		{
			"class": "qxl.logpane.Application",
			"theme": "qx.theme.Modern",
			"name": "logview",
			"title": "Qooxdoo Log View"
		}
	],
	/** Targets */
	"targets": [
		{
			"type": "source",
			"outputPath": "source-output"
		}
	],
	"defaultTarget": "source",
	"locales": [
		"en", "de", "ro", "sv"
	],
	"serve": {
	   "listenPort": 8080
	}
}